#!/usr/bin/env node

// uppy-server start

var program = require('commander')
var server = require('../server2')

var version = require('../package.json').version

program
  .version(version)
  .option('-p, --port', 'specify a port', '4000')
  .option('-c, --config', 'pass a config file')
  .action(function () {
    server()
  })
  .parse(process.argv)
