{
  "name": "uppy-server",
  "version": "0.0.3",
  "description": "Almost as cute as a Puppy :dog:",
  "main": "index.js",
  "bin": "index.js",
  "scripts": {
    "fix": "eslint . --fix",
    "launch": "source env.infra.sh && ${FREYCMD:-frey} infra --project-dir ./infra",
    "console": "source env.sh && ${FREYCMD:-frey} remote --project-dir ./infra",
    "deploy:travis": "bash -c 'source env.sh && env DEBUG=\"\" frey setup --force-yes --project-dir ./infra'",
    "deploy": "source env.sh && ${FREYCMD:-frey} setup --project-dir ./infra",
    "release:major": "env SEMANTIC=major npm run release",
    "release:minor": "env SEMANTIC=minor npm run release",
    "release:patch": "env SEMANTIC=patch npm run release",
    "release": "npm version ${SEMANTIC:-patch} -m \"Release %s\" && git push && git push --tags && npm publish",
    "lint": "eslint .",
    "start": "source env.sh && env UPPYSERVER_DOMAIN=localhost UPPY_ENDPOINT=http://localhost:4000 node index.js",
    "start:production": "node index.js"
  },
  "author": "Transloadit.com",
  "license": "ISC",
  "homepage": "https://github.com/transloadit/uppy-server#readme",
  "devDependencies": {
    "eslint": "2.4.0",
    "eslint-config-standard": "5.1.0",
    "eslint-plugin-standard": "1.3.2",
    "fakefile": "0.0.8",
    "frey": "0.3.15",
    "nodemon": "1.8.1"
  },
  "dependencies": {
    "dropbox": "0.10.3",
    "google-auth-library": "0.9.7",
    "googleapis": "2.1.7",
    "grant-koa": "3.5.3",
    "instagram-node": "0.5.8",
    "koa": "1.1.2",
    "koa-cors": "0.0.16",
    "koa-mount": "1.3.0",
    "koa-qs": "2.0.0",
    "koa-router": "5.3.0",
    "koa-session": "3.3.1",
    "purest": "2.0.0"
  }
}
