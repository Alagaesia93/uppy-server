{
  "extends": "standard",
  "env": {
    "browser": true,
    "node": false
  },
  "globals": {
    "window": false
  },
  "rules": {
    "key-spacing": [2, { "align": "colon" }],
    "no-multi-spaces": [2, { "exceptions": { "Property": true, "VariableDeclarator": true, "ImportDeclaration": true}}]
  }
}
